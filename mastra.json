{
  "name": "smart-auth-orchestrator",
  "version": "1.0.0",
  "description": "Smart Authentication Orchestrator - A Mastra template for intelligent authentication",
  "type": "mastra",
  "framework": "mastra",
  "language": "typescript",
  "entry": "src/mastra/index.ts",
  "build": {
    "command": "npm run build",
    "output": "dist/"
  },
  "dev": {
    "command": "mastra dev",
    "port": 3000
  },
  "components": {
    "agents": "src/mastra/agents/",
    "tools": "src/mastra/tools/",
    "workflows": "src/mastra/workflows/"
  },
  "demo": {
    "file": "demo.html",
    "description": "Interactive authentication demo"
  }
}
